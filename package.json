{
  "name": "@pascaliske/eslint-config",
  "description": "Shared ESLint config for TypeScript projects.",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/eslint-config.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslintrc",
    "eslintconfig",
    "config",
    "configuration"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "test": "eslint --config dist/index.js src/index.ts",
    "format": "prettier --write \"src/*.ts\"",
    "changelog": "standard-changelog"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^2.12.0",
    "eslint-config-prettier": "^6.7.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "eslint": "^6.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@pascaliske/tsconfig": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "standard-changelog": "^2.0.21",
    "typescript": "^3.7.3"
  }
}
